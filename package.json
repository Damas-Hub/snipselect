{
  "name": "snipselect",
  "displayName": "SnipSelect",
  "description": "Select multiple snippets to paste later",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "snipselect.openPanel",
        "title": "SnipSelect: Open Panel"
      }
    ]
  },
  "scripts": {
    "compile": "esbuild src/extension.ts --bundle --outdir=dist --platform=node",
    "watch": "esbuild src/extension.ts --bundle --outdir=dist --platform=node --watch"
  },
  "devDependencies": {
    "esbuild": "^0.17.0",
    "typescript": "^4.9.4",
    "@types/vscode": "^1.70.0"
  }
}
